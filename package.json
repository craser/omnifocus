{
  "name": "omnifocus",
  "version": "1.0.0",
  "description": "## This provides a way to quickly create OmniFocus tasks using either the command line or Alfred.",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && rollup -c --environment NODE_ENV:production",
    "build:dev": "npm run clean && rollup -c",
    "test": "jest",
    "watch": "rollup -c -w",
    "webhooks:start": "node dist/js/webhooks.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github-raserio:craser/omnifocus.git"
  },
  "author": "chris@raser.io",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.25.7",
    "@babel/preset-env": "^7.25.7",
    "@rollup/plugin-alias": "^5.1.1",
    "eslint": "^9.12.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.31.0",
    "jest": "^29.7.0",
    "jsonwebtoken": "^9.0.2",
    "rimraf": "^6.0.1",
    "rollup": "^2.79.2",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-executable": "^1.6.3",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "supertest": "^7.1.1"
  },
  "dependencies": {
    "body-parser": "^2.2.0",
    "express": "^5.1.0",
    "openai": "^5.0.1",
    "xml2js": "^0.6.2"
  },
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./*"
      ],
      "~/*": [
        "./*"
      ]
    }
  }
}
